{

  "is_debug": true,

  "form_name": "ozn-sample",
  "form_root": "/document/sample_form/index.php",

  "unload_message": "このページから移動してよろしいですか？\n入力内容は保存されていない可能性があります。",

  "mail": {

    "auto_reply": true,

    "send_by": "Gmail SMTP",

    "admin_mail_to": "oznform+admin_mail@gmail.com",
    "admin_mail_cc": [],
    "admin_mail_bcc": [],

    "customer_address_form_name": "email",
    "customer_mail_cc": [],
    "customer_mail_bcc": ["oznform+for_customer@gmail.com"],

    "from_name": "OznFormテスト送信者",
    "from_address": "oznform@gmail.com",

    "redirect_to": ""
  },

  "pages": {

    "index": {
      "role": "form",
      "forms": {

        "title": {
          "label": "お問い合わせ種別",
          "validates": ["required"]
        },
        "corporate_name": {
          "label": "企業名・団体名",
          "validates": ["required"]
        },
        "corporate_kana": {
          "label": "企業名・団体名フリガナ",
          "validates": ["required", "kanaOnly"]
        },
        "customer_name": {
          "label": "ご担当者様氏名",
          "validates": ["required"]
        },
        "customer_kana": {
          "label": "ご担当者様フリガナ",
          "validates": ["required", "kanaOnly"]
        },

        "email": {
          "label": "メールアドレス",
          "validates": ["required", "email"]
        },

        "tel": {
          "label": "電話番号",
          "validates": ["required", "tel"]
        },
        "fax": {
          "label": "FAX番号",
          "validates": ["tel"]
        },

        "zip-code": {
          "label": "郵便番号",
          "validates": ["zip"]
        },
        "prefecture": {
          "label": "都道府県",
          "validates": ["required"]
        },
        "street-address": {
          "label": "番地まで",
          "validates": ["required"]
        },
        "address-level4": {
          "label": "建物名等"
        },

        "shipping-date": {
          "label": "ご希望納期"
        },

        "mail_body": {
          "label": "お問い合わせ詳細",
          "validates": ["required"]
        },

        "materials": {
          "label": "資料送付",
          "validates": ["required"],
          "error_message_position": ".materials",
          "error_message_template": "<div class=\"col-sm-12\"><% messages %></div>",
          "error_messages": {
            "required": "送付する資料を選んでください"
          }
        },



        "shipping-zip-code1": {
          "label": "送付先 郵便番号（３桁）",
          "validates": ["required"],
          "error_message_position": ".shipping-zip-area",
          "error_message_template": "<div class=\"col-sm-12\"><% messages %></div>"
        },
        "shipping-zip-code2": {
          "label": "送付先 郵便番号（４桁）",
          "validates": ["required"],
          "error_message_position": ".shipping-zip-area",
          "error_message_template": "<div class=\"col-sm-12\"><% messages %></div>"
        },
        "shipping-address1": {
          "label": "送付先 住所１（番地まで）",
          "validates": ["required"]
        },
        "shipping-address2": {
          "label": "送付先 住所２（建物名など）"
        },

        "privacy": {
          "label": "個人情報の取り扱い",
          "validates": ["required"],
          "error_message_position": ".privacy-error",
          "error_message_template": "<p><% messages %></p>",
          "error_messages": {
            "required": "お問い合わせにあたっては、個人情報の取り扱いへの同意をお願いします。"
          }
        }
      }
    },

    "confirm": {
      "role": "confirm"
    },

    "complete": {
      "role": "mailsend"
    }
  }
}